<odoo>
    <data>
        <record id="leap_property_tag_view_action" model="ir.actions.act_window">
            <field name="name">Property tag</field>
            <field name="res_model">leap.property.tag</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="leap_property_tag_view_tree" model="ir.ui.view">
            <field name="name">leap.property.tag.view.tree</field>
            <field name="model">leap.property.tag</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
